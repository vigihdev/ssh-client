{
    "name": "vigihdev/ssh-client",
    "version": "1.0.0",
    "description": "A lightweight and robust SSH client for PHP to easily connect, execute commands, and manage remote shells.",
    "type": "library",
    "keywords": [
        "php",
        "ssh",
        "client",
        "remote-command",
        "shell"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "require": {
        "php": "^8.0",
        "phpseclib/phpseclib": "*",
        "symfony/filesystem": "^6.4",
        "symfony/finder": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\Ssh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Ssh\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "vigihdev/symfony-bridge-config": "^1.0",
        "symfony/console": "^6.4",
        "vigihdev/encryption": "dev-main"
    },
    "scripts": {
        "test": "phpunit",
        "server": "php -S localhost:1134 server.php"
    }
}
